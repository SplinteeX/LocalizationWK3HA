<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.wk3localizationha.ProductController">
    <children>
        <ComboBox fx:id="languageComboBox" layoutX="20.0" layoutY="20.0" />
        <Button fx:id="loadButton" layoutX="200.0" layoutY="20.0" text="Load Products" />
        <TableView fx:id="productTable" layoutX="20.0" layoutY="60.0" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" />
                <TableColumn fx:id="nameColumn" text="Name" />
                <TableColumn fx:id="descriptionColumn" text="Description" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
